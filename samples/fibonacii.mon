
let fibonacii = fn(n) {
    if (n == 0) {
        return 0;
    }
    if (n == 1) {
        return 1;
    }
    return fibonacii(n - 1) + fibonacii(n - 2);
};

puts("calculate nth fibonacii number :")
puts("--------------------------------")

let n = input("enter n: ");
puts("result =", fibonacii(n))